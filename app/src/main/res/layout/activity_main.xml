        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/backImg"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="@drawable/img_ten_back"/>
        <ImageView
            android:id="@+id/undoImg"
