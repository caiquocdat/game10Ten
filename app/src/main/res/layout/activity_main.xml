        android:layout_marginBottom="80dp"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:gravity="center"
            android:layout_centerVertical="true"
            android:background="@drawable/brg_point"
            android:layout_width="120dp"
            android:layout_height="50dp">
            <TextView
                android:layout_centerVertical="true"
                android:id="@+id/lableTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Point"
                android:textSize="12sp"
                android:textColor="@color/white"
                />
            <TextView
                android:id="@+id/textPoint"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:layout_toRightOf="@+id/lableTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="10"
                android:textSize="20sp"
                android:textColor="@color/white"
                />

        </RelativeLayout>
        <RelativeLayout
            android:layout_alignParentRight="true"
            android:id="@+id/rankRel"
            android:gravity="center"
            android:background="@drawable/brg_point"
            android:layout_width="120dp"
            android:layout_height="50dp">
            <TextView
                android:layout_centerVertical="true"
                android:id="@+id/lableRankTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Highest"
                android:textSize="12sp"
                android:textColor="@color/white"
                />
            <TextView
                android:id="@+id/maxPointTv"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:layout_toRightOf="@+id/lableRankTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="100"
                android:textSize="20sp"
                android:textColor="@color/white"
                />

